<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<script name="Set Horn CopyPaste Filter">
    <shortcut command="ctrl shift alt Z" enable="1" in-menu="1" />
    <lines>
        <line content="// need to ensure filter is on" />
        <line content='menu item &quot;Edit Filter...&quot;' />
        <line content='click &quot;None&quot;' />
        <line content='check &quot;Notes and rests&quot;' />
        <line content='check &quot;Articulations&quot;' />
        <line content='check &quot;Expressions: Dynamics, Expressive Text, Technique Text &amp; Miscellaneous&quot;' />
        <line content='check &quot;Smart Shapes (assigned to notes)&quot;' />
        <line content="" />
        <line content='check &quot;Special alterations&quot;' />
        <line content="" />
        <line content='click &quot;Ok&quot;' />
        <line content=" " />
    </lines>
</script>
